<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzone.xservice.app.dao.MccCodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xzone.xinterface.app.model.MccCode">
        <result column="CODE" property="code" />
        <result column="MERCHANT_NAME" property="merchantName" />
        <result column="MERCHANT_RATE_NEW" property="merchantRateNew" />
        <result column="MERCHANT_RATE_OLD" property="merchantRateOld" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CODE, MERCHANT_NAME, MERCHANT_RATE_NEW, MERCHANT_RATE_OLD
    </sql>

</mapper>
